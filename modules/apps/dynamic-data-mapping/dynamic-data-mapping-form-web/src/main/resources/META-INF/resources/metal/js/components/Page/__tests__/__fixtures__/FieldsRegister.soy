{namespace FieldsRegister}

{deltemplate FormRenderer.RegisterFieldType variant="'radio'"}
	{call Fields.render data="all"}
		{param type: 'radio' /}
	{/call}
{/deltemplate}

{deltemplate FormRenderer.RegisterFieldType variant="'text'"}
	{call Fields.render data="all"}
		{param type: 'text' /}
	{/call}
{/deltemplate}

{deltemplate FormRenderer.RegisterFieldType variant="'select'"}
	{call Fields.render data="all"}
		{param type: 'select' /}
	{/call}
{/deltemplate}